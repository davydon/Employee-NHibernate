<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="CompanyDetailsNHibernate" namespace="CompanyDetailsNHibernate.Models">
  <class name="Staffs" table="Staffs" dynamic-update="true">
    <cache usage="read-write" />
    <id name="Id" column="Id" type="int">
      <generator class="native"/>
    </id>

    <property name="Surname"/>
    <property name="Firstname"/>
    <property name="Address"/>
    <property name="Phone"/>
    <property name="Email"/>
    <property name="Salary"/>
  </class>
</hibernate-mapping>
